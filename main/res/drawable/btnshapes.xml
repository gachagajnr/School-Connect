<?xml version="1.0" encoding="utf-8" ?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
  <item android:state_pressed="false" >
    <shape>
      <solid
          android:color="#ffffff" />
      <stroke
          android:width="1dp"
          android:color="#000000" />
      <corners
          android:radius="15dp" />
      <padding
          android:left="5dp"
          android:top="5dp"
          android:right="5dp"
          android:bottom="5dp" />
    </shape>
  </item>

</selector>